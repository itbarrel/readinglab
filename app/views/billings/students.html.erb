<% content_for :search_includes do %>
  <%= render partial: 'shared/search', locals: { search_key: :first_name_cont } %>
<% end %>

<div class="card mb-3" data-list='{"valueNames":["name","strating date","ending date","vacation type","action"],"page":15}'>
  <div class="card-header">
    <div class="row flex-between-center">
      <div class="col-4 col-sm-auto d-flex align-items-center pe-0">
          <h5 class="fs-0 mb-0 text-nowrap py-2 py-xl-0">Students</h5>
      </div>
    </div>
  </div>

  <div class="card-body p-0" id="studentsTable">
    <%= render partial: 'table', locals: { students: @students } %>
  </div>
</div>
