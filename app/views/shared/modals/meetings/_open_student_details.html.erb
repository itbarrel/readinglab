<%= form_with(model: Form, url: student_detail_meeting_path(@meeting), method: :get) do |form| %>

  <div class="modal-header px-card bg-light border-bottom-0">
    <h5 class="modal-title"><%= title %></h5>
    <button class="btn-close me-n1" type="button" data-bs-dismiss="modal" aria-label="Close"></button>
  </div>

  <div class="mb-3">
    <%= form.select :name, @meeting.forms.pluck(:name), class: ".form-select-sm example" %>
    <%= form.submit 'Submit', class: "btn btn-primary" %>
  </div>

  <div class="card-footer d-flex justify-content-end align-items-center bg-light">
    <%= form.submit 'Save', class: 'btn btn-primary px-4' %>
    <%= form.button 'Cancel', class: 'btn btn-default px-4', type: 'button', data: { 'bs-dismiss': 'modal'} %>
  </div>
<% end %>
