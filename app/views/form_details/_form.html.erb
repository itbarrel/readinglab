<%= form_with(model: form_detail) do |form| %>
  <% if form_detail.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(form_detail.errors.count, "error") %> prohibited this form_detail from being saved:</h2>

      <ul>
        <% form_detail.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :form_values, style: "display: block" %>
    <%= form.text_field :form_values %>
  </div>

  <div>
    <%= form.label :active, style: "display: block" %>
    <%= form.check_box :active %>
  </div>

  <div>
    <%= form.label :deleted_at, style: "display: block" %>
    <%= form.datetime_field :deleted_at %>
  </div>

  <div>
    <%= form.label :parent_types, style: "display: block" %>
    <%= form.text_field :parent_types %>
  </div>

  <div>
    <%= form.label :user_id, style: "display: block" %>
    <%= form.text_field :user_id %>
  </div>

  <div>
    <%= form.label :form_id, style: "display: block" %>
    <%= form.text_field :form_id %>
  </div>

  <div>
    <%= form.label :account_id, style: "display: block" %>
    <%= form.text_field :account_id %>
  </div>

  <div>
    <%= form.label :parent_id, style: "display: block" %>
    <%= form.text_field :parent_id %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
